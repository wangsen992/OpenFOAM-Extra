FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    floor
    {
        type            nutkWallFunction;
        z0              uniform 1;
        value           uniform 0;
    }
    ceiling
    {
        type            zeroGradient;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    leftBoundary
    {
        type            cyclic;
    }
    rightBoundary
    {
        type            cyclic;
    }
}


