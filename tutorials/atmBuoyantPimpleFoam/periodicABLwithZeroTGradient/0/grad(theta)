FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      grad(theta);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    floor
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    ceiling
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    leftBoundary
    {
        type            cyclic;
    }
    rightBoundary
    {
        type            cyclic;
    }
}


// ************************************************************************* //
