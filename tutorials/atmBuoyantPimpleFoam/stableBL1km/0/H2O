FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.;

boundaryField
{
    floor
    {
        type            fixedGradient;
        gradient           uniform 0.;
    }

    ceiling
    {
        type            fixedValue;
        value           $internalField;
    }

    "(inlet|outlet|leftBoundary|rightBoundary)"
    {
        type            cyclic;
    }
}


// ************************************************************************* //
