FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      l;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    "(floor|ceiling)"
    {
        type            calculated;
        value           uniform 0.1;
    }
    "(inlet|outlet|leftBoundary|rightBoundary)"
    {
        type            cyclic;
    }
}


