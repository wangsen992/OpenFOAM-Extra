FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      q;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    "(floor|wall)"
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
    ceiling
    {
        type            zeroGradient;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    leftBoundary
    {
        type            cyclic;
    }
    rightBoundary
    {
        type            cyclic;
    }
}


