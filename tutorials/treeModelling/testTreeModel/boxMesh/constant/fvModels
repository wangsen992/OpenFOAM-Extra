FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

geostrophicForce
{
    type            geostrophicForce;

    selectionMode   all;

		UName						U;
		Ug							(10 0 0);
		f								(0 0 1e-4);
}

boussinesqBuoyancyForce
{
		type						boussinesqBuoyancyForce;

		selectionMode		all;

		UName						U;
		theta0					300;
		g								(0 0 -10);
}


